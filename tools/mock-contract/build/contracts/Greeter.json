  "source": "pragma solidity ^0.6.0;\n\ncontract Greeter {\n\n    string greeting;\n\n    constructor(string memory _greeting) public {\n        greeting = _greeting;\n    }\n\n    function greet() public view returns (string memory) {\n        return greeting;\n    }\n\n    function setGreeting(string memory _greeting) public {\n        greeting = _greeting;\n    }\n\n}",
